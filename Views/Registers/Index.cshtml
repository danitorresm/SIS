
<h1>Historial de Registros</h1>
@foreach (var Register in Model){


<div class="card" style="width: 18rem;">
    <div class="card-body">
        <h1>Employees.Name</h1>
        <h5 class="card-title">Entrada</h5>
        <h6 class="card-subtitle mb-2 text-body-secondary">@Register.HoraEntrada</h6>
        <h5 class="card-title">Salida</h5>
        <h6 class="card-subtitle mb-2 text-body-secondary">@Register.HoraSalida</h6>
    </div>
</div>
}